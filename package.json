{
  "name": "zsecure-express",
  "version": "1.0.0",
  "description": "Enterprise-grade security layer for Express.js with advanced protection, deception, and threat intelligence",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --coverage",
    "test:security": "jest test/security.test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --production",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run build && npm run test && npm run audit"
  },
  "keywords": [
    "security",
    "express",
    "middleware",
    "honeypot",
    "firewall",
    "threat-detection",
    "waf",
    "rate-limiting",
    "authentication",
    "encryption",
    "csrf",
    "xss",
    "sql-injection",
    "deception",
    "owasp"
  ],
  "author": "logien",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zsecure/express-security"
  },
  "bugs": {
    "url": "https://github.com/zsecure/express-security/issues"
  },
  "homepage": "https://zsecure.dev",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.0",
    "express-validator": "^7.0.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": false
    }
  },
  "optionalDependencies": {
    "ioredis": "^5.0.0"
  },
  "sideEffects": false
}